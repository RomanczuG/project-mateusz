    "body": "\r\n    const assert = require('assert'),\r\n      cloudinary = require('cloudinary'),\r\n      corruptImage = path.resolve(__dirname, 'corrupted.png'),\r\n      options = {\r\n        width: 720,\r\n        height: 1000,\r\n        resource_type: \"auto\",\r\n        crop: \"limit\"\r\n      };\r\n\r\n    cloudinary.config({\r\n      cloud_name: \"\",\r\n      api_key: \"\",\r\n      api_secret: \"\",\r\n    });\r\n\r\n\r\n    it(\"should error since file is corrupt\", (done) => {\r\n      cloudinary.v2.uploader.upload(corruptImage, options, function(err, result) {\r\n        assert.ok(err);\r\n        assert.ifError(result);\r\n      });\r\n    });\r\n\r\n    it(\"should error since file is corrupt\", (done) => {\r\n      let readable, writable;\r\n      readable = fs.createReadStream(corruptImage);\r\n      writable = cloudinary.v2.uploader.upload_stream(options, function(err, result) {\r\n        assert.ok(err);\r\n        assert.ifError(result); // error\r\n      });\r\n      readable.pipe(writable);\r\n    });\r\n",
